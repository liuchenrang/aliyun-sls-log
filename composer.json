{
    "name": "yoctometre/aliyun-sls",
    "description": "AliYun SLS Log For Hyperf",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "aliyun",
        "sls"
    ],
    "authors": [
        {
            "name": "xiaoyin199",
            "email": "xiaoyin199@126.com"
        }
    ],
    "require": {
        "hyperf/guzzle": "^3.0.0",
        "hyperf/contract": "^3.0.0",
        "monolog/monolog": "^2.7|^3.1",
        "ext-swoole": "*",
        "php": ">=8.0"
    },
    "require-dev": {
        "hyperf/config": "^3.0.0",
        "hyperf/event": "^3.0.0",
        "hyperf/framework": "^3.0.0",
        "hyperf/process": "^3.0.0",
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Ym\\AliyunSls\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YmTest\\AliyunSls\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Ym\\AliyunSls\\ConfigProvider"
        }
    }
}
